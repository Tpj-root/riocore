{
    "name": "Spartan6",
    "description": "Spartan6 - only for testing the toolchain",
    "boardcfg": "Spartan6",
    "machinetype": "mill",
    "protocol": "SPI",
    "plugins": [
        {
            "type": "spi",
            "pins": {
                "mosi": {
                    "pin": "p83"
                },
                "miso": {
                    "pin": "p82"
                },
                "sclk": {
                    "pin": "p84"
                },
                "sel": {
                    "pin": "p81"
                }
            }
        },

        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "LED:LED1",
                    "modifier": [
                        {
                            "type": "onerror"
                        }
                    ]
                }
            },
            "frequency": 1.0
        },
        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "LED:LED2",
                    "modifier": [
                        {
                            "type": "onerror"
                        }
                    ]
                }
            },
            "frequency": 5.0
        },


        {
            "type": "bitout",
            "name": "led00",
            "pins": {
                "bit": {
                    "pin": "p79"
                }
            }
        },
        {
            "type": "bitout",
            "name": "led01",
            "pins": {
                "bit": {
                    "pin": "p80"
                }
            }
        },
        {
            "type": "bitout",
            "name": "led02",
            "pins": {
                "bit": {
                    "pin": "p74"
                }
            }
        },
        {
            "type": "bitout",
            "name": "led03",
            "pins": {
                "bit": {
                    "pin": "p78"
                }
            }
        }

    ]
}
