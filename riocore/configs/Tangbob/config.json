{
    "name": "Tangbob",
    "boardcfg": "Tangbob",
    "description": "Tangbob with one 5x china BOB",
    "protocol": "UDP",
    "plugins": [
        {
            "type": "w5500",
            "pins": {
                "mosi": {
                    "pin": "SPI:MOSI"
                },
                "miso": {
                    "pin": "SPI:MISO"
                },
                "sclk": {
                    "pin": "SPI:SCLK"
                },
                "sel": {
                    "pin": "SPI:SEL"
                }
            }
        },
        {
            "type": "wled",
            "pins": {
                "data": {
                    "pin": "WLED:DATA"
                }
            }
        },
        {
            "type": "modbus",
            "pins": {
                "tx": {
                    "pin": "modbus:TX"
                },
                "rx": {
                    "pin": "modbus:RX"
                },
                "tx_enable": {
                    "pin": "modbus:TX_ENABLE"
                }
            }
        }
    ],
    "modules": [
        {
            "slot": "RC",
            "module": "china-bob5x",
            "setup": {
                "enable": {
                    "type": "bitout",
                    "name": "enable",
                    "pins": {
                        "bit": {
                            "modifier": [
                                {
                                    "type": "invert"
                                },
                                {
                                    "type": "onerror"
                                },
                                {
                                    "type": "invert"
                                }
                            ]
                        }
                    }
                },
                "home-x": {
                    "type": "bitin",
                    "name": "home-x",
                    "pins": {
                        "bit": {
                            "pull": "up"
                        }
                    },
                    "signals": {
                        "bit": {
                            "net": "joint.0.home-sw-in",
                            "function": "",
                            "setp": ""
                        }
                    }
                },
                "home-y": {
                    "type": "bitin",
                    "name": "home-y",
                    "pins": {
                        "bit": {
                            "pull": "up"
                        }
                    },
                    "signals": {
                        "bit": {
                            "net": "joint.1.home-sw-in",
                            "function": "",
                            "setp": ""
                        }
                    }
                },
                "home-z": {
                    "type": "bitin",
                    "name": "home-z",
                    "pins": {
                        "bit": {
                            "pull": "up"
                        }
                    },
                    "signals": {
                        "bit": {
                            "net": "joint.2.home-sw-in",
                            "function": "",
                            "setp": ""
                        }
                    }
                },
                "spindle-enable": {
                    "type": "bitout",
                    "name": "spindle-enable",
                    "pins": {
                        "bit": {
                            "modifier": [
                                {
                                    "type": "invert"
                                }
                            ]
                        }
                    },
                    "signals": {
                        "bit": {
                            "net": "spindle.0.on",
                            "function": "",
                            "setp": ""
                        }
                    }
                },
                "pwm": {
                    "type": "pwmout",
                    "name": "pwm",
                    "pins": {
                        "pwm": {
                            "modifier": [
                                {
                                    "type": "invert"
                                }
                            ]
                        }
                    }
                },
                "e-stop": {
                    "type": "bitin",
                    "name": "e-stop",
                    "pins": {
                        "bit": {
                            "pull": "up"
                        }
                    }
                },
                "probe": {
                    "type": "bitin",
                    "name": "probe",
                    "pins": {
                        "bit": {
                            "pull": "up"
                        }
                    }
                },
                "joint-0": {
                    "name": "joint-0",
                    "type": "stepdir",
                    "is_joint": true
                },
                "joint-1": {
                    "name": "joint-1",
                    "type": "stepdir",
                    "is_joint": true
                },
                "joint-2": {
                    "name": "joint-2",
                    "type": "stepdir",
                    "is_joint": true
                },
                "joint-3": {
                    "name": "joint-3",
                    "type": "stepdir",
                    "is_joint": true
                }
            }
        }
    ]
}