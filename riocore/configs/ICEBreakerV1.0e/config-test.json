{
    "name": "ICEBreakerV1.0e",
    "description": "Small and low cost FPGA educational and development board",
    "boardcfg": "ICEBreakerV1.0e",
    "modules": [
        {
            "slot": "PMOD_2",
            "module": "w5500"
        }
    ],
    "plugins": [
        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "LED:G",
                    "modifier": [
                        {
                            "type": "onerror"
                        }
                    ]
                }
            },
            "frequency": 1.0
        },
        {
            "type": "modbus",
            "pins": {
                "tx": {
                    "pin": "PMOD_1A:P1"
                },
                "rx": {
                    "pin": "PMOD_1A:P2"
                },
                "tx_enable": {
                    "pin": "PMOD_1A:P3"
                }
            },
            "baud": 9600,
            "rx_buffersize": 128,
            "tx_buffersize": 128,
            "signals": {
                "temperature": {
                    "display": {}
                }
            }
        }
    ],
    "transport": "UDP",
    "axis": 3
}