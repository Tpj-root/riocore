{
    "name": "ICEBreakerV1.0e",
    "description": "Small and low cost FPGA educational and development board",
    "boardcfg": "ICEBreakerV1.0e",
    "machinetype": "mill",
    "protocol": "UDP",
    "modules": [
        {
            "slot": "PMOD_2",
            "module": "w5500"
        }
    ],
    "plugins": [
        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "LED:G",
                    "modifier": [
                        {
                            "type": "onerror"
                        }
                    ]
                }
            },
            "frequency": 1.0
        },
        {
            "name": "joint-x",
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "PMOD_1A:P2"
                },
                "dir": {
                    "pin": "PMOD_1A:P3"
                }
            },
            "is_joint": true,
            "joint": {
                "scale": 800.0
            }
        },
        {
            "name": "joint-y",
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "PMOD_1A:P4"
                },
                "dir": {
                    "pin": "PMOD_1B:P10"
                }
            },
            "is_joint": true,
            "joint": {
                "scale": -800.0
            }
        },
        {
            "name": "joint-z",
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "PMOD_1B:P9"
                },
                "dir": {
                    "pin": "PMOD_1B:P8"
                }
            },
            "is_joint": true,
            "joint": {
                "scale": -1600.0
            }
        },
        {
            "type": "bitin",
            "name": "home_x",
            "pins": {
                "bit": {
                    "pin": "PMOD_1B:P4",
                    "pullup": true,
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            },
            "signals": {
                "bit": {
                    "net": "joint.0.home-sw-in"
                }
            }
        },
        {
            "type": "bitin",
            "name": "home_y",
            "pins": {
                "bit": {
                    "pin": "PMOD_1B:P2",
                    "pullup": true,
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            },
            "signals": {
                "bit": {
                    "net": "joint.1.home-sw-in"
                }
            }
        },
        {
            "type": "bitin",
            "name": "home_z",
            "pins": {
                "bit": {
                    "pin": "PMOD_1A:P8",
                    "pullup": true,
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            },
            "signals": {
                "bit": {
                    "net": "joint.2.home-sw-in"
                }
            }
        },
        {
            "type": "bitin",
            "name": "din1",
            "pins": {
                "bit": {
                    "pin": "PMOD_1B:P3",
                    "pullup": true,
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        }
    ],
    "axis": 3
}
