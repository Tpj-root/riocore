{
    "name": "rio-icebreaker3x",
    "description": "ICEBreakerV1.0e with rio-icebreaker3x voard",
    "boardcfg": "icebreaker3x",
    "machinetype": "mill",
    "protocol": "SPI",
    "plugins": [
        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "LED:G",
                    "modifier": [
                        {
                            "type": "onerror"
                        }
                    ]
                }
            },
            "frequency": 1.0
        },
        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "LED:R",
                    "modifier": [
                        {
                            "type": "onerror",
                            "invert": true
                        }
                    ]
                }
            },
            "frequency": 2.0
        },
        {
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "JOINT1:STEP"
                },
                "dir": {
                    "pin": "JOINT1:DIR"
                }
            },
            "is_joint": true,
            "name": "Stepper1"
        },
        {
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "JOINT2:STEP"
                },
                "dir": {
                    "pin": "JOINT2:DIR"
                }
            },
            "is_joint": true,
            "name": "Stepper2"
        },
        {
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "JOINT3:STEP"
                },
                "dir": {
                    "pin": "JOINT3:DIR"
                }
            },
            "is_joint": true,
            "name": "Stepper3"
        },
        {
            "type": "spi",
            "pins": {
                "mosi": {
                    "pin": "SPI:MOSI"
                },
                "miso": {
                    "pin": "SPI:MISO"
                },
                "sclk": {
                    "pin": "SPI:SCLK"
                },
                "sel": {
                    "pin": "SPI:SEL"
                }
            }
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "IN1:BIT",
                    "pull": "up",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "IN2:BIT",
                    "pull": "up",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "IN3:BIT",
                    "pull": "up",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "IN4:BIT",
                    "pull": "up",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "IN5:BIT",
                    "pull": "up",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitout",
            "pins": {
                "bit": {
                    "pin": "OUT1:BIT",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitout",
            "pins": {
                "bit": {
                    "pin": "OUT2:BIT",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitout",
            "name": "enable",
            "pins": {
                "bit": {
                    "pin": "4",
                    "modifier": [
                        {
                            "type": "invert"
                        },
                        {
                            "type": "onerror"
                        }
                    ]
                }
            }
        }
    ]
}