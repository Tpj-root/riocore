{
    "name": "CYC1000",
    "boardcfg": "CYC1000",
    "plugins": [
        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "LED:D1"
                }
            }
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "SW:S1",
                    "pull": "up"
                }
            }
        },
        {
            "type": "bitout",
            "pins": {
                "bit": {
                    "pin": "LED:D2"
                }
            }
        },
        {
            "type": "bitout",
            "pins": {
                "bit": {
                    "pin": "LED:D3"
                }
            }
        },
        {
            "type": "pwmout",
            "pins": {
                "pwm": {
                    "pin": "LED:D4"
                },
                "dir": {
                    "pin": "LED:D5"
                },
                "en": {
                    "pin": "LED:D6"
                }
            }
        },
        {
            "type": "freqout",
            "pins": {
                "freq": {
                    "pin": "LED:D7"
                }
            }
        },
        {
            "type": "freqout",
            "pins": {
                "freq": {
                    "pin": "LED:D8"
                }
            }
        },
        {
            "type": "uart",
            "pins": {
                "rx": {
                    "pin": "FTDI:BDBUS0"
                },
                "tx": {
                    "pin": "FTDI:BDBUS1"
                }
            }
        }
    ],
    "linuxcnc": {
        "probe_screen": {
            "enable": false
        }
    }
}
