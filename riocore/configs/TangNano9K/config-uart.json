{
    "name": "TangNano9K",
    "description": "TangNano9K over UART for testing basic functions",
    "boardcfg": "TangNano9K",
    "protocol": "UART",
    "plugins": [
        {
            "type": "uart",
            "pins": {
                "rx": {
                    "pin": "18"
                },
                "tx": {
                    "pin": "17"
                }
            }
        },
        {
            "type": "bitout",
            "name": "blink",
            "pins": {
                "bit": {
                    "pin": "10",
                    "modifier": [
                        {
                            "type": "invert"
                        },
                        {
                            "type": "pwm"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitout",
            "name": "spindle",
            "pins": {
                "bit": {
                    "pin": "11",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            },
            "signals": {
                "bit": {
                    "net": "spindle.0.on"
                }
            }
        },
        {
            "type": "bitin",
            "name": "in1",
            "pins": {
                "bit": {
                    "pin": "3",
                    "pullup": true,
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "bitin",
            "name": "in2",
            "pins": {
                "bit": {
                    "pin": "4",
                    "pullup": true,
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            }
        },
        {
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "13",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                },
                "dir": {
                    "pin": "14",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            },
            "is_joint": true
        },
        {
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "15",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                },
                "dir": {
                    "pin": "16",
                    "modifier": [
                        {
                            "type": "invert"
                        }
                    ]
                }
            },
            "is_joint": true
        }
    ]
}
